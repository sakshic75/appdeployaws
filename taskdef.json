{
    "family": "demoapp-defintion",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::914221124747:role/service-role/codebuild-project-test-service-role",
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "containerDefinitions": [
       
        {
            "name": "demoapp",
            "image": "<IMAGE_NAME>",
            "essential":true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "protocol": "tcp"
                }
            ]
           
        }
    ]
    
   
}