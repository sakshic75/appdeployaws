{
    "family": "demoapp-defintion",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::aws:policy/service-role/AmazonECSTaskExecutionRolePolicy",
    "requiresCompatibilities" : ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "<APP_NAME>",
            "image": "<IMAGE_NAME>",
            "essential":true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "protocol": "tcp"
                }
            ]
           
            
        },
        {
            "name": "<DB_NAME>",
            "image": "<IMAGE_DB_NAME>",
            "essential":true,
            "portMappings": [
                {
                    "containerPort": 1433,
                    "protocol": "tcp"
                }
            ]
           
        }
    ]
    
   
}